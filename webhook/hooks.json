[
	{
		"id": "content-pull-webhook",
		"execute-command": "/usr/local/shared/git-pull.sh",
		"command-working-directory": "/home/webhook",
		"trigger-rule": {
			"match": {
				"type": "payload-hash-sha1",
				"secret": "no-secrets-here",
				"parameter": {
					"source": "header",
					"name": "X-Hub-Signature"
				}
			}
		}
	},
	{
		"id": "theme-pull-webhook",
